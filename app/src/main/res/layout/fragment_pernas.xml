                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:textSize="30dp"
                    android:text="Agachamento Livre"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/txtDesc10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:textSize="20dp"
                    android:text="Com barra"
                    android:textAlignment="center" />
            </LinearLayout>
        </LinearLayout>



        <LinearLayout
            android:id="@+id/linear11"
            android:background="@drawable/divisao_partes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imageView11"
                android:src="@drawable/stiff_com_halteres"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_marginStart="10dp"
                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtTitulo11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:textSize="30dp"
                    android:text="Stiff"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/txtDesc11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:textSize="20dp"
                    android:text="Com Hateres"
                    android:textAlignment="center" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.core.widget.NestedScrollView>
